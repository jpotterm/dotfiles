{
    "css.completion.completePropertyWithSemicolon": false,

    "dart.closingLabels": false,
    "[dart]": {
        "editor.tabSize": 4,
        "editor.detectIndentation": true,
    },

    "editor.acceptSuggestionOnEnter": "off",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.colorDecorators": false,
    "editor.cursorBlinking": "solid",
    "editor.dragAndDrop": false,
    "editor.find.globalFindClipboard": false,
    "editor.find.seedSearchStringFromSelection": false,
    "editor.fontFamily": "Roboto Mono",
    "editor.fontSize": 11,
    "editor.hideCursorInOverviewRuler": true,
    "editor.lineHeight": 18,
    "editor.lineNumbers": "off",
    "editor.occurrencesHighlight": false,
    "editor.parameterHints.enabled": false,
    "editor.roundedSelection": false,
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "none",
    "editor.wordWrap": "on",

    "emmet.excludeLanguages": [
        "markdown",
        "css",
        "scss",
    ],
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",

    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,

    "extensions.ignoreRecommendations": true,

    "files.associations": {
        "*.sls": "yaml"
    },
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        ".expo": true,
        ".vagrant": true,
        "**/__pycache__": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/*.orig": true,
        "**/*.pyc": true,
        "**/CVS": true,
        "**/node_modules": true
    },
    "files.hotExit": "off",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,

    "git.decorations.enabled": false,

    "go.lintOnSave": "off",
    "go.useLanguageServer": true,
    "go.vetOnSave": "off",

    "html.autoClosingTags": false,
    "html.validate.scripts": false,
    "html.validate.styles": false,

    "macros.list": {
        "closeOtherTabs": [
            "workbench.action.closeOtherEditors",
            "workbench.files.action.collapseExplorerFolders",
            "workbench.files.action.showActiveFileInExplorer",
            "workbench.action.focusFirstEditorGroup",
        ],
    },

    "python.disableInstallationCheck": true,
    "python.linting.enabled": false,
    "python.testing.promptToConfigure": false,
    "python.workspaceSymbols.enabled": false,

    "scm.diffDecorations": "gutter",

    "scss.completion.completePropertyWithSemicolon": false,

    "search.exclude": {},
    "search.quickOpen.includeHistory": false,

    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,

    "window.newWindowDimensions": "maximized",
    "window.restoreWindows": "none",
    "window.zoomLevel": 0,

    "workbench.colorCustomizations": {
        "activityBar.background": "#E8E8E8",
        "activityBar.foreground": "#777",
        "activityBarBadge.background": "#777",
        "editorGroupHeader.tabsBackground": "#E8E8E8",
        "editorIndentGuide.background": "#00000009",
        "editorIndentGuide.activeBackground": "#00000018",
        "editorLineNumber.activeForeground": "#555",
        "editorOverviewRuler.border": "#00000000",
        "editorWidget.background": "#E8E8E8",
        "editorWidget.border": "#00000000",
        "scrollbar.shadow": "#00000000",
        "sideBar.background": "#E8E8E8",
        "statusBar.background": "#E8E8E8",
        "statusBar.debuggingBackground": "#E8E8E8",
        "statusBar.debuggingForeground": "#777",
        "statusBar.foreground": "#777",
        "statusBar.noFolderBackground": "#E8E8E8",
        "statusBar.noFolderForeground": "#777",
        "tab.border": "#E8E8E8",
        "tab.inactiveBackground": "#E8E8E8",
        "titleBar.activeBackground": "#E8E8E8",
        "widget.shadow": "#00000000",
    },
    "workbench.colorTheme": "Quiet Light",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.fontAliasing": "antialiased",
    "workbench.iconTheme": null,
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 20,
    "timeline.excludeSources": [],
    "python.defaultInterpreterPath": "/usr/local/bin/python3",
    "python.showStartPage": false,
    "debug.javascript.codelens.npmScripts": "never",
    "breadcrumbs.filePath": "off",
    "breadcrumbs.icons": false,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "go.toolsManagement.autoUpdate": true,
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.untitled.hint": "hidden",
    "editor.minimap.enabled": false,
    "security.workspace.trust.enabled": false,
}
